<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- Color Display -->
		<dict>
			<key>cell</key> 				<string>PSLinkCell</string>
			<key>cellClass</key> 			<string>PFSimpleLiteColorCell</string>
			<key>libcolorpicker</key>
			<dict>
				<key>defaults</key> 		<string>com.kayfam.hueprefs</string>
				<key>key</key> 				<string>tint_color</string>
				<key>fallback</key> 		<string>#007aff</string>
				<!-- <key>PostNotification</key> <string>com.kayfam.hueprefs/settingschanged</string> -->
				<key>alpha</key> 			<true/>
			</dict>
			<key>label</key> 				<string>Current Color</string>
		</dict>

		<dict>
			<key>cell</key> 				<string>PSLinkListCell</string>
			<key>label</key> 				<string>Theme</string>
			<key>detail</key> 				<string>PSListItemsController</string>
			<key>defaults</key> 			<string>com.kayfam.hueprefs</string>

			<key>key</key> 					<string>tint_color</string>
			<key>default</key> 				<integer>0</integer>

			<key>titlesDataSource</key> 	<string>themeTitles</string>
			<key>valuesDataSource</key> 	<string>themeValues</string>
		</dict>
	</array>
</dict>
</plist>